<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:padding="@dimen/fab_margin" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginBottom="64dp">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView android:layout_width="match_parent" android:layout_height="wrap_content" app:cardBackgroundColor="@color/cardview_shadow_start_color">
                <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="20sp" android:textStyle="bold" android:textColor="@android:color/white" android:id="@+id/textView" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:layout_marginBottom="8dp" android:text="National Diploma: Engineering: Computer Systems; Vaal University of Technology: 2018" android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
                    <ImageView android:id="@+id/vut_logo" android:background="@drawable/vut_front" android:layout_width="match_parent" android:layout_height="200dp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView android:layout_width="match_parent" android:layout_height="wrap_content" app:cardBackgroundColor="@color/cardview_shadow_start_color">
                <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="20sp" android:textStyle="bold" android:textColor="@android:color/white" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:layout_marginBottom="8dp" android:text="Gade 12: National Senior Certificate; Futura High: 2012" android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
                    <ImageView android:id="@+id/futura_logo" android:background="@drawable/futura_logo" android:layout_width="match_parent" android:layout_height="250dp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <TextView android:textSize="20sp" android:textStyle="bold" android:textColor="@android:color/white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:text="Skills"/>
            <RelativeLayout android:padding="@dimen/fab_margin" android:layout_width="match_parent" android:layout_height="wrap_content">
                <LinearLayout android:id="@+id/php" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_alignParentTop="true" android:layout_alignParentStart="true">
                    <ImageView android:id="@+id/php_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/html5" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_alignParentTop="true" android:layout_centerInParent="true">
                    <ImageView android:id="@+id/html5_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/css3" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_alignParentTop="true" android:layout_alignParentEnd="true">
                    <ImageView android:id="@+id/css3_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/c_prog" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/php">
                    <ImageView android:id="@+id/c_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/c_sharp" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/html5" android:layout_centerInParent="true">
                    <ImageView android:id="@+id/cs_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/wordpress" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/css3" android:layout_alignParentEnd="true">
                    <ImageView android:id="@+id/wp_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/mysql" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/c_prog">
                    <ImageView android:id="@+id/mysql_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/angular" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/c_sharp" android:layout_centerInParent="true">
                    <ImageView android:id="@+id/angular_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/js" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/wordpress" android:layout_alignParentEnd="true">
                    <ImageView android:id="@+id/js_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/jquery" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/mysql">
                    <ImageView android:id="@+id/jquery_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/laravel" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/angular" android:layout_centerInParent="true">
                    <ImageView android:id="@+id/laravel_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/xd" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/js" android:layout_alignParentEnd="true">
                    <ImageView android:id="@+id/xd_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/bootstrap" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/jquery">
                    <ImageView android:id="@+id/bootstrap_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/github" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/laravel" android:layout_centerInParent="true">
                    <ImageView android:id="@+id/github_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/photoshop" android:background="@drawable/skills_bg" android:padding="11dp" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="@dimen/fab_margin" android:layout_marginBottom="@dimen/fab_margin" android:layout_below="@+id/xd" android:layout_alignParentEnd="true">
                    <ImageView android:id="@+id/ps_logo" android:layout_width="45dp" android:layout_height="45dp"/>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</androidx.core.widget.NestedScrollView>
